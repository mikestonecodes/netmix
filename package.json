{
  "name": "staart-example",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node index.js",
    "build": "next build",
    "start": "export NODE_ENV=prod && node index.js",
    "linked-modules": "find node_modules/ -maxdepth 1 -type l -ls | grep -o -E \"[^/]+$\"",
    "save-linked-modules": "yarn -s linked-modules > .linked_modules",
    "link-modules": "cat .linked_modules | xargs yarn link",
    "docker:build": "docker build -t staart .",
    "docker:clean": "docker rm -f staart || true",
    "docker:run": "docker run -p 7880:3000 -d --link staart-mongo:mongo --name staart staart && yarn docker:logs",
    "docker:stop": "docker stop staart",
    "docker:start": "docker start staart && yarn docker:logs",
    "docker:logs": "docker logs -f staart",
    "deploy": "yarn docker:build && yarn docker:clean && yarn docker:run"
  },
  "author": "Nicola Marcacci Rossi <nicolamr@gmail.com> (http://nmr.io)",
  "license": "ISC",
  "dependencies": {
    "compose-next": "^1.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-server-express": "^1.2.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.10.0",
    "grid-styled": "^3.2.0",
    "mailcomposer": "^4.0.2",
    "mailgun-js": "^0.13.1",
    "mongodb": "^2.2.33",
    "next": "^4.0.0-beta.6",
    "ooth": "^1.1.0",
    "ooth-client": "^1.1.0",
    "ooth-client-react": "^1.1.0",
    "ooth-client-react-next": "^1.1.1",
    "ooth-client-react-next-apollo": "^1.1.1",
    "ooth-facebook": "^1.1.0",
    "ooth-google": "^1.1.0",
    "ooth-local": "^1.1.0",
    "ooth-mongo": "^1.1.0",
    "react": "^16.1.1",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.1.1",
    "react-image": "^1.3.1",
    "react-player": "^1.1.2",
    "react-rating": "^1.0.5",
    "react-ratings-declarative": "3.0.1",
    "react-search-input": "^0.11.3",
